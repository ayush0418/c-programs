#include<stdio.h>
void main()
{
         int n,r=0,d,t;

	 printf("\n enter any number ");
	 scanf("%d",&n);
          
	 t=n;

	 while(t!=0)
	 {
	     d = t%10;
	     r = r*10+d;
	     t = t/10;
	 }

	 if(n==r)
	 {
	        printf("\n %d is a pallindrom number.",n)
	    else
	        printf("\n %d is not a pallindrom number.",n);
         }
}


/*  OUTPUT OF PROGRAM

    enter any number 123
    123 is a pallindrom number.
*/
