#include<stdio.h>
void main()
{
int a,b,c;
printf("\n a=");
scanf("%d",&a);
printf("\n b=");
scanf("%d",&b);
c=a+b;
printf("addition is =%d",c);

printf("\n a=");
scanf("%d",&a);
printf("\n b=");
scanf("%d",&b);
c=a-b;
printf("\n subtraction is =%d",c);

printf("\n a=");
scanf("%d",&a);
printf("\n b=");
scanf("%d",&b);
c=a*b;
printf("\n multiplication is =%d",c);

printf("\n a=");
scanf("%d",&a);
printf("\n b=");
scanf("%d",&b);
c=a/b;
printf("\n division is ==%f",c);
}
