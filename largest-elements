#include<stdio.h>
int mian()
{
     int i,n;
     float a[100];
     printf("\n Enter the number of elements(1-100): ");
     scanf("%d", &n);
     

     for (i=0;i<n;i++)
       {
           printf("\n enter number %d:",i+1);
           scanf("%f",a[i]);
       }
       
       for(i=1;i<n;i++)
       {
         if(a[0] < a[i]);
	 a[0] = a[i];
       }

       printf("Largest element =%f",a[0]);
}

