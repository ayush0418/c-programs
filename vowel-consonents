#include<stdio.h>
int main()
{
   char c;

   printf("\n enter the character");
   scanf("%c",&c);
   if(c=='a' || c=='A' || c=='e' || c=='E' || c=='i' || c=='I' || c=='o' || c=='O' || c=='u' || c=='U')
   {
         printf("\n character entered is vowel");
     else
         printf("\n character entered is consonent");
   }
}

