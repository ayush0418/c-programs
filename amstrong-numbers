#include<stdio.h>
int main()
{
     int n,r=0,d,t;    
     printf("\n enter any number: ");
     scanf("%d",&n);

     t=n;
     while(t!=0)
     {
           d = t%10;            //"d" is the remainder
	   r = r + d*d*d;       //"r" is the result
	   t = t/10;            //"t" is the original number
     }
     
     if(r==n)
     {
          printf("\n %d is armstrong",n);
     else
          printf("\n %d is not armstrong",n);
     }
}
