#include<stdio.h>
int main()
{
     int n,r=0,d,t;
     printf("\n enter any number: ");
     scanf("%d",&n);

     t=n;
     while(t!=0)
     {
           d = t%10;
	   r = r + d*d*d;
	   t = t/10;
     }
     
     if(r==n)
     {
          printf("\n %d is armstrong",n);
     else
          printf("\n %d is not armstrong",n);
     }
}
