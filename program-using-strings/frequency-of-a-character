#include<stdio.h>
int main()
{
   char str[100],ch;
   int count = 0,i;

   printf("\n Enter a string: ");
   gets(str);

   printf("\n Enter a character to find its frequency");
   scanf("%d",&ch);

   for(i=0;str[i]!=\0;i++)
   {
     if(ch==str[i])
       ++count;
   }

   printf("\n Frequency of %c is: %d",ch,count);
}
