#include<stdio.h.
int mian()
{
    int n1,n2,gcd;
    printf("\n enter the first number: ");
    scanf("%d",&n1);
    printf("\n enter the second number: ");
    scanf("%d",&n2);

    for(i=1;i<=n1 && i<=n2;++i)
    {
       if(n2%i==0 && n2%i==0)
         gcd=i;
    }
    printf("\n G.C.D of %d and %d is %d"n1,n2,gcd);
}

